{
    "name": "@swatbloc/sdk",
    "version": "1.1.1",
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "description": "SDK for SwatBloc headless commerce - access products, cart, and checkout",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --tsconfig tsconfig.build.json",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "clean": "rm -rf dist",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "swatbloc",
        "ecommerce",
        "headless",
        "sdk",
        "cursor",
        "lovable"
    ],
    "author": "SwatBloc",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/qadolphe/MegaPlatform.git",
        "directory": "packages/sdk"
    },
    "devDependencies": {
        "tsup": "^8.0.0",
        "typescript": "^5.0.0"
    }
}